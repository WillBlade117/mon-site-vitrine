---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import Services from '../components/Services.astro';
import Portfolio from '../components/Portfolio.astro';
import Faq from '../components/Faq.astro';

const pageTitle = "Création de Sites Web Haute Performance | William SART";
const pageDescription = "Développeur web indépendant spécialisé Astro & Next.js. Je conçois des sites vitrines ultra-rapides et des applications sur-mesure pour transformer vos visiteurs en clients.";
---

<Layout title={pageTitle} description={pageDescription}>
    <main>
        <Hero />
        <Services />
        <Portfolio />
        <Faq />
    </main>
</Layout>